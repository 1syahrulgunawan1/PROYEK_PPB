<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".ValActivity"
    tools:layout_editor_absoluteY="81dp">



    <TextView
        android:id="@+id/textView"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/tv_data2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/tv_data2"
        android:text="RENTAL PS  BERKAH"
        android:textAlignment="center"
        android:textSize="19dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tv_data1"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nama" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button1"
        android:layout_alignRight="@+id/button1"
        android:layout_below="@+id/tv_data1"
        android:layout_marginTop="14dp"
        android:text="Bookingan PS mu pada :"
        android:textAppearance="?android:attr/textAppearanceMedium"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="82dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tv_data2"
        android:layout_alignBottom="@+id/tv_data2"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:text="Tanggal :"
        android:textAppearance="?android:attr/textAppearanceMedium"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="114dp" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tv_data3"
        android:layout_alignBottom="@+id/tv_data3"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:text="Jam :"
        android:textAppearance="?android:attr/textAppearanceMedium"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="169dp" />


    <EditText
        android:id="@+id/tv_data1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_data2"
        android:layout_alignStart="@+id/tv_data2"
        android:layout_below="@+id/textView"
        android:layout_marginTop="33dp"
        android:text="Data 1"
        android:editable="false"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <EditText
        android:id="@+id/tv_data2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_data3"
        android:layout_alignStart="@+id/tv_data3"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="26dp"
        android:text="Data 2"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:editable="false"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="113dp" />

    <EditText
        android:id="@+id/tv_data3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button1"
        android:layout_alignStart="@+id/button1"
        android:layout_below="@+id/tv_data2"
        android:layout_marginTop="19dp"
        android:text="Data 3"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:editable="false"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="169dp" />
    <EditText
        android:id="@+id/tv_data4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button1"
        android:layout_alignStart="@+id/button1"
        android:layout_below="@+id/tv_data3"
        android:layout_marginTop="19dp"
        android:text="Data 4"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:editable="false"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="169dp" />



    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button2"
        android:layout_alignLeft="@+id/button2"
        android:layout_alignStart="@+id/button2"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="16dp"
        android:text="Kembali"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="352dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="12dp"
        android:text="Exit"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="413dp" />


</RelativeLayout>